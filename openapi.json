{
  "openapi": "3.1.0",
  "info": {
    "title": "SDK-IT API",
    "version": "1.0.0"
  },
  "servers": [
    {
      "url": "/",
      "description": "Same host"
    },
    {
      "url": "http://localhost:3000",
      "description": "Local Server"
    }
  ],
  "security": [
    {
      "bearer": []
    }
  ],
  "paths": {
    "/publish": {
      "post": {
        "operationId": "postPublish",
        "parameters": [],
        "tags": [],
        "description": "",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "required": ["specUrl"],
                "type": "object",
                "properties": {
                  "specUrl": {
                    "type": "string",
                    "format": "uri"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Response for 200",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "message": {
                      "enum": ["SDK published successfully"],
                      "type": "string"
                    },
                    "specUrl": {
                      "type": "string"
                    }
                  },
                  "required": ["message", "specUrl"],
                  "additionalProperties": false
                }
              }
            }
          },
          "400": {
            "description": "Bad Request",
            "content": {
              "application/json": {
                "schema": {
                  "oneOf": [
                    {
                      "$ref": "#/components/schemas/ValidationError"
                    }
                  ]
                }
              }
            }
          }
        }
      }
    },
    "/augment": {
      "post": {
        "operationId": "postAugment",
        "parameters": [],
        "tags": [],
        "description": "Annotate the spec with additional information like code snippets, pagination, descriptions, examples, etc.\nusing LLM.",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "required": ["specUrl"],
                "type": "object",
                "properties": {
                  "specUrl": {
                    "type": "string",
                    "format": "uri"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "400": {
            "description": "Bad Request",
            "content": {
              "application/json": {
                "schema": {
                  "oneOf": [
                    {
                      "$ref": "#/components/schemas/ValidationError"
                    }
                  ]
                }
              }
            }
          }
        }
      }
    },
    "/fetch": {
      "get": {
        "operationId": "getFetch",
        "parameters": [
          {
            "in": "query",
            "name": "url",
            "required": true,
            "schema": {
              "type": "string",
              "format": "uri"
            }
          }
        ],
        "tags": [],
        "description": "",
        "responses": {
          "200": {
            "description": "Response for 200",
            "content": {
              "application/json": {
                "schema": {}
              }
            }
          },
          "400": {
            "description": "Bad Request",
            "content": {
              "application/json": {
                "schema": {
                  "oneOf": [
                    {
                      "$ref": "#/components/schemas/ValidationError"
                    }
                  ]
                }
              }
            }
          }
        }
      }
    },
    "/generate": {
      "post": {
        "operationId": "postGenerate",
        "parameters": [],
        "tags": [],
        "description": "",
        "requestBody": {
          "required": true,
          "content": {
            "multipart/form-data": {
              "schema": {
                "required": ["specFile"],
                "type": "object",
                "properties": {
                  "specFile": {
                    "type": "string",
                    "format": "binary"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Response for 200",
            "content": {
              "text/plain": {
                "schema": {
                  "type": "string"
                }
              }
            },
            "headers": {
              "Transfer-Encoding": {
                "schema": {
                  "type": "string",
                  "enum": ["chunked"]
                }
              }
            }
          },
          "400": {
            "description": "Bad Request",
            "content": {
              "application/json": {
                "schema": {
                  "oneOf": [
                    {
                      "$ref": "#/components/schemas/ValidationError"
                    }
                  ]
                }
              }
            }
          }
        }
      }
    },
    "/playground": {
      "post": {
        "operationId": "postPlayground",
        "parameters": [],
        "tags": ["playground"],
        "description": "",
        "requestBody": {
          "required": true,
          "content": {
            "multipart/form-data": {
              "schema": {
                "required": ["specFile"],
                "type": "object",
                "properties": {
                  "specFile": {
                    "type": "string",
                    "format": "binary"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Response for 200",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "url": {
                      "enum": [
                        "https://raw.githubusercontent.com/openai/openai-openapi/refs/heads/master/openapi.yaml"
                      ],
                      "type": "string"
                    },
                    "title": {
                      "type": "string"
                    },
                    "name": {
                      "type": "string"
                    },
                    "clientName": {
                      "type": "string"
                    }
                  },
                  "required": ["url", "title", "name", "clientName"],
                  "additionalProperties": false
                }
              }
            }
          },
          "400": {
            "description": "Bad Request",
            "content": {
              "application/json": {
                "schema": {
                  "oneOf": [
                    {
                      "$ref": "#/components/schemas/ValidationError"
                    }
                  ]
                }
              }
            }
          }
        }
      }
    },
    "/operations": {
      "get": {
        "operationId": "getOperations",
        "parameters": [
          {
            "in": "query",
            "name": "page",
            "required": true,
            "schema": {
              "type": "integer",
              "minimum": 1,
              "default": 1
            }
          },
          {
            "in": "query",
            "name": "pageSize",
            "required": true,
            "schema": {
              "type": "integer",
              "minimum": 1,
              "maximum": 100,
              "default": 20
            }
          }
        ],
        "tags": ["operations"],
        "description": "",
        "responses": {
          "200": {
            "description": "Response for 200",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "operations": {
                      "type": "array",
                      "items": {
                        "type": "string"
                      }
                    },
                    "pagination": {
                      "type": "object",
                      "properties": {
                        "hasNextPage": {
                          "type": "boolean"
                        },
                        "hasPreviousPage": {
                          "type": "boolean"
                        }
                      },
                      "required": ["hasNextPage", "hasPreviousPage"],
                      "additionalProperties": false
                    }
                  },
                  "required": ["operations", "pagination"],
                  "additionalProperties": false
                }
              }
            }
          },
          "400": {
            "description": "Bad Request",
            "content": {
              "application/json": {
                "schema": {
                  "oneOf": [
                    {
                      "$ref": "#/components/schemas/ValidationError"
                    }
                  ]
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "ValidationError": {
        "type": "object",
        "properties": {
          "message": {
            "type": "string"
          }
        }
      }
    },
    "securitySchemes": {
      "bearer": {
        "type": "http",
        "scheme": "bearer"
      }
    }
  }
}
