{
  "name": "@sdk-it/core",
  "version": "0.36.1",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./*": {
      "types": "./dist/lib/*",
      "import": "./dist/lib/*",
      "default": "./dist/lib/*"
    }
  },
  "files": [
    "dist",
    "!**/*.tsbuildinfo",
    "!**/*.test.*"
  ],
  "dependencies": {
    "debug": "^4.4.0",
    "stringcase": "^4.3.1",
    "lodash-es": "^4.17.21",
    "zod-to-json-schema": "^3.24.6"
  },
  "peerDependencies": {
    "typescript": "^5.8.3",
    "openapi3-ts": "^4.4.0",
    "zod": "^3.25.76 || ^4.0.0"
  },
  "devDependencies": {
    "@types/debug": "^4.1.12"
  }
}
