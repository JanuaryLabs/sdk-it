{
  "name": "@sdk-it/spec",
  "version": "0.17.0",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./*": {
      "types": "./dist/lib/*",
      "import": "./dist/lib/*",
      "default": "./dist/lib/*"
    }
  },
  "files": [
    "dist",
    "!**/*.tsbuildinfo"
  ],
  "dependencies": {
    "openapi3-ts": "4.4.0",
    "yaml": "^2.7.0",
    "stringcase": "^4.3.1"
  }
}
