{
  "name": "@sdk-it/spec",
  "version": "0.30.0",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./*": {
      "types": "./dist/lib/*",
      "import": "./dist/lib/*",
      "default": "./dist/lib/*"
    }
  },
  "files": [
    "dist",
    "!**/*.tsbuildinfo"
  ],
  "dependencies": {
    "@sdk-it/core": "0.30.0",
    "openapi3-ts": "4.5.0",
    "pluralize": "^8.0.0",
    "stringcase": "^4.3.1",
    "yaml": "^2.7.0",
    "fast-content-type-parse": "^3.0.0",
    "lodash-es": "^4.17.21",
    "debug": "^4.4.1",
    "micromatch": "4.0.8",
    "p-limit": "3.1.0"
  }
}
